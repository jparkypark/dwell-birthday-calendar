{
  "name": "dwell-birthday-calendar",
  "version": "1.0.0",
  "description": "A lightweight Slack application that displays member birthdays for Dwell Church",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler publish",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:performance": "vitest run tests/performance"
  },
  "keywords": ["slack", "birthday", "cloudflare-workers", "church"],
  "author": "Dwell Church",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240925.0",
    "@types/node": "^22.7.9",
    "typescript": "^5.6.3",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^9.13.0",
    "vitest": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "@vitest/coverage-v8": "^1.0.4",
    "miniflare": "^3.20240925.0"
  },
  "dependencies": {
    "itty-router": "^5.0.18"
  }
}